<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title">Edits</h3>
	</div>
	<div class="panel-body">
		<div class="table-responsive">
			<table class="table table-striped table-hover">
				<thead>
					<tr>
						<th>Edit Id</th>
						<th>Type</th>
						<th>Date Submitted</th>
						<th><a class="sort" data-sort="Edit.status" {@eq key="{sort.sort}" value="Collectible.status"}data-direction="{sort.direction}" {/eq}>Status</a></th>
					</tr>
				</thead>
				<tbody>

				</tbody>
			</table>
		</div>	
	</div>
	<div class="panel-footer">
		{{#showPagination}}
		<ul class="_pagination"></ul>
		{{/showPagination}}		
	</div>
</div>