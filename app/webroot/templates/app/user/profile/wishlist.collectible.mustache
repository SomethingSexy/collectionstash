<div class="image">
	<span class="label label-info">{{{Collectible.displayTitle}}}</span>
	{{#Collectible.CollectiblesUpload}}
		{{#primary}}
			<a data-gallery="gallery" href="{{Upload.url}}" class="">
				<img class="" src="{{Upload.thumbnailUrl}}" resizetype="strict" alt="{{Collectible.descriptionTitle}}">
			</a>
		{{/primary}}
	{{/Collectible.CollectiblesUpload}}
	{{^Collectible.CollectiblesUpload}}
		<a target="_blank" href="javascript:void(0);" class=""><img alt="" src="/img/no-photo.png"></a>
	{{/Collectible.CollectiblesUpload}}

	<div class="tile-actions bottom-right">
		<div class="btn-group">
			<a class="btn btn-default" title="View Collectible Details" href="/collectibles/view/{{Collectible.id}}"><i class="fa fa-info"></i></a>
			{{#permissions.edit_collectible_user}}
				<a data-prompt="true" data-collectible-user-id="{{id}}" class="remove-from-wishlist btn btn-default" title="Remove" href="#"><i class="fa fa-trash-o"></i></a>
				<a class="add-full-to-stash btn btn-default" data-type="wishlist" data-collectible-user-id="{{id}}"  href="javascript:void(0)" title="Add to Stash"><img src="/img/icon/add_stash_link_16x16.png"></a>
			{{/permissions.edit_collectible_user}}
		</div>
	</div>
</div>

