<div class="thumbnail">
	{{#Collectible.CollectiblesUpload}}
		{{#primary}}
			<a data-gallery="gallery" href="/files/{{Upload.name}}" class="thumbnail">
				<img class="img-responsive" src="/files/{{Upload.name}}" resizetype="strict" alt="{{Collectible.descriptionTitle}}">
			</a>
		{{/primary}}
	{{/Collectible.CollectiblesUpload}}
	{{^Collectible.CollectiblesUpload}}
		<a target="_blank" href="/collectibles_users/view/{{id}}" class="thumbnail"><img alt="" src="/img/no-photo.png"></a>
	{{/Collectible.CollectiblesUpload}}
	<div class="caption">
	<h3>
	{{#Collectible.custom}}
		<a class="title" target="_blank" href="/collectibles/view/{{Collectible.id}}">{{{Collectible.displayTitle}}}</a>
	{{/Collectible.custom}}
	{{#Collectible.original}}
		<a class="title" target="_blank" href="/collectibles/view/{{Collectible.id}}">{{{Collectible.displayTitle}}}</a>
	{{/Collectible.original}}
	{{^Collectible.original}}{{^Collectible.custom}}
		<a class="title" target="_blank" href="/collectibles_users/view/{{id}}">{{{Collectible.displayTitle}}}</a>
	{{/Collectible.custom}}{{/Collectible.original}}
	</h3>

	<div class="_actions">
		<span><a class="" title="View Collectible Details" href="/collectibles/view/{{Collectible.id}}"><i class="icon-info"></i></a></span>
		{{#permissions.edit_collectible_user}}
			<span><a class="add-full-to-stash btn" data-type="wishlist" data-collectible-user-id="{{id}}"  href="javascript:void(0)" title="Add to Stash"><img src="/img/icon/add_stash_link_25x25.png"></a></span>
			<span><a data-prompt="true" data-collectible-user-id="{{id}}" class="remove-from-wishlist" title="Remove" href="#"><i class="icon-trash"></i></a></span>
		{{/permissions.edit_collectible_user}}
	</div>
	</div>

</div>
