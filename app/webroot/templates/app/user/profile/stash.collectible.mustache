	<div class="image">
	{{#UserUpload}}
			<a data-gallery="gallery" href="{{UserUpload.imagePath}}" class="">
				<img class="" src="{{UserUpload.thumbnailUrl}}" resizetype="strict" alt="{{UserUpload.description}}">
			</a>
	{{/UserUpload}}
	{{^UserUpload}}{{#Collectible.CollectiblesUpload}}
		{{#primary}}
			<a data-gallery="gallery" href="{{Upload.url}}" class="">
				<img class="" src="{{Upload.thumbnailUrl}}" resizetype="strict" alt="{{Collectible.descriptionTitle}}">
			</a>
		{{/primary}}
	{{/Collectible.CollectiblesUpload}}
	{{^Collectible.CollectiblesUpload}}
		<a target="_blank" href="/collectibles_users/view/{{id}}" class=""><img alt="" src="/img/no-photo.png"></a>
	{{/Collectible.CollectiblesUpload}}
	{{/UserUpload}}
	</div>
	<div class="caption">
		<h3>
		{{#Collectible.custom}}
			<a class="title" target="_blank" href="/collectibles/view/{{Collectible.id}}">{{{Collectible.displayTitle}}}</a>
		{{/Collectible.custom}}
		{{#Collectible.original}}
			<a class="title" target="_blank" href="/collectibles/view/{{Collectible.id}}">{{{Collectible.displayTitle}}}</a>
		{{/Collectible.original}}
		{{^Collectible.original}}{{^Collectible.custom}}
			<a class="title" target="_blank" href="/collectibles_users/view/{{id}}">{{{Collectible.displayTitle}}}</a>
		{{/Collectible.custom}}{{/Collectible.original}}
		</h3>
		<div class="_actions menu tile-links clearfix adjust">
			<span class="pull-left marked-for-sale {{^sale}}hidden{{/sale}}">
			<a class="" title="Marked for sale"><i class="fa fa-dollar"></i></a></span>
			<span><a class="" title="View Collectible Details" href="/collectibles/view/{{Collectible.id}}"><i class="fa fa-info"></i></a></span>
			{{#permissions.edit_collectible_user}}
			<span><a href="javascript:void(0);" class="_add-photo"><i class="fa fa-camera"></i></a></span>
				<span><a class="" title="Edit" href="/collectibles_users/edit/{{id}}"><i class="fa fa-edit"></i></a></span>
				{{^sale}}
					<span><a href="javascript:void(0);" data-collectible-user-id="{{id}}" class="stash-sell" title="Sell"><i class="fa fa-dollar"></i></a></span>
				{{/sale}}
				<span><a data-prompt="true" data-collectible-user-id="{{id}}" class="remove-from-stash" title="Remove" href="#"><i class="fa fa-trash-o"></i></a></span>
			{{/permissions.edit_collectible_user}}
		</div>
	</div>
